<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<meta content="text/html; charset=utf-8" http-equiv="Content-Type"/>
<title>{{ html_title() }}</title>
<link href="themes/2008/css/styles_global.css" rel="stylesheet" type="text/css"/>
<link href="themes/2008/css/styles_home.css" rel="stylesheet" type="text/css"/>
<link href="themes/2008/css/styles_tabs.css" rel="stylesheet" type="text/css"/>
<link href="themes/2008/css/header.css" rel="stylesheet" type="text/css"/>
<script src="themes/2008/js/main.js" type="text/javascript"></script>
<script src="themes/2008/js/swfobject.js" type="text/javascript"></script>
<script src="themes/2008/js/prototype.js" type="text/javascript"></script>
<script src="themes/2008/js/jserrorreport.js" type="text/javascript"></script>
<script src="themes/2008/js/header.js" type="text/javascript"></script>
</head>
<body>
<!-- Main Background -->
<div id="all">
<!-- Header -->
{{ header_2008() }}
<div id="cart_status_row">
<center>
<div id="cart_status_center">
<div id="cart_status_data">
{{ cart_status() }}
</div>
</div>
</center>
</div>
<!-- End Header -->
<center>
<div id="col_parent">
<br clear="all"/>
<!-- Left Column -->
<div id="col_left">
{{ sidebar_2008_search() }}
{{ sidebar_2008_browse() }}
{{ sidebar_2008_install_steam() }}
{{ sidebar_2008_freeloads() }}
{{ sidebar_2008_news() }}
{{ sidebar_2008_catalogs() }}
{{ sidebar_2008_steam_info() }}
</div>
<!-- End Left Column -->
<!-- Center Column -->
<div id="col_center">
<div class="error_page">
<h1 class="error_page__title">{{ page_title|default('Error') }}</h1>
<div class="error_page__body">
{{ error_html|default(content|default(''))|raw }}
</div>
</div>
</div>
<!-- End Center Column -->
<!-- Right Column -->
<div id="col_right">
{{ sidebar_right_2008() }}
</div>
<!-- End Right Column -->
<br clear="all">
</br></div>
</center>
<!-- Footer -->
{{ footer_2008() }}
<!-- End Footer -->
</div>
<!-- End Main Background -->
<script language="JavaScript">
{{ tab_switching_script() }}
</script>
</body>
</html>
