{# 2005 storefront body #}
<table align="center" cellpadding="0" cellspacing="0">
<tr>
<td bgcolor="#FFFFFF" style="width: 10px; height: 10px;" valign="top"><img src="img\storefront\sp_top_left.gif"/></td>
<td bgcolor="#FFFFFF" rowspan="3" valign="top">
<table align="center" border="0" cellpadding="0" cellspacing="0" style="height: 100%;" width="736">
<tr>
<td align="left" nowrap="" valign="top" width="10"></td>
<td align="left" nowrap="" valign="top" width="118">
<table cellpadding="0" cellspacing="0">
<tr>
<td height="10"></td>
</tr>
<tr>
<td nowrap="">
{% for ln in links %}
  {% if ln.type == 'spacer' %}
    <div class="menu_spacer"></div>
  {% elseif ln.hidden is empty %}
    {% if ln.current is not empty %}
      <span><font face="Wingdings 3"><span class="menu_pointer">&#132;</span></font> <a class="menu_item_current" href="{{ ln.url }}">{{ ln.label }}</a></span><br/>
    {% else %}
      <span><a class="menu_item" href="{{ ln.url }}">{{ ln.label }}</a></span><br/>
    {% endif %}
  {% endif %}
{% endfor %}
</td>
</tr>
</table>
</td>
<td width="8"><nobr></nobr></td>
<td align="left" style="height: 100%" valign="top" width="100%">
<table cellpadding="0" cellspacing="0" width="100%">
<tr>
<td valign="top">
<br/>
<div style="position: relative; width: 590px; height: 511px;">
  <!-- Top Game Image -->
  {% if capsules.top is defined and capsules.top %}
  <a href="{{ BASE }}/storefront/index.php?area=game&AppId={{ capsules.top.appid }}">
    <img src="{{ BASE }}/storefront/images/capsules/{{ capsules.top.image }}"
         style="position: absolute; left: 1px; top: 1px; width: 588px; height: 98px; border: 0;"
         alt="Top capsule" border="0"/>
  </a>
  {% endif %}
  
  <!-- Horizontal Bar 1 -->
  <img src="{{ BASE }}/storefront/images/capsules/2006_08-August/horizontal_top_middle.png"
       style="position: absolute; left: 0; top: 99px; width: 590px; height: 13px; border: 0;" 
       border="0"/>
  
  <!-- Middle Game Image -->
  {% if capsules.middle is defined and capsules.middle %}
  <a href="{{ BASE }}/storefront/index.php?area=game&AppId={{ capsules.middle.appid }}">
    <img src="{{ BASE }}/storefront/images/capsules/{{ capsules.middle.image }}"
         style="position: absolute; left: 0; top: 112px; width: 589px; height: 228px; border: 0;"
         alt="Middle capsule" border="0"/>
  </a>
  {% endif %}
  
  <!-- Horizontal Bar 2 -->
  <img src="{{ BASE }}/storefront/images/capsules/2006_08-August/horizontal_bar_lower.png"
       style="position: absolute; left: 0; top: 340px; width: 590px; height: 12px; border: 0;" 
       border="0"/>
  
  <!-- Bottom-Left Game Image -->
  {% if capsules.bottom_left is defined and capsules.bottom_left %}
  <a href="{{ BASE }}/storefront/index.php?area=game&AppId={{ capsules.bottom_left.appid }}">
    <img src="{{ BASE }}/storefront/images/capsules/{{ capsules.bottom_left.image }}"
         style="position: absolute; left: 0; top: 352px; width: 288px; height: 158px; border: 0;"
         alt="Bottom left capsule" border="0"/>
  </a>
  {% endif %}
  
  <!-- Vertical Bar -->
  <img src="{{ BASE }}/storefront/images/capsules/2006_08-August/btm_middle_bar.png"
       style="position: absolute; left: 288px; top: 352px; width: 13px; height: 158px; border: 0;" 
       border="0"/>
  
  <!-- Bottom-Right Game Image -->
  {% if capsules.bottom_right is defined and capsules.bottom_right %}
  <a href="{{ BASE }}/storefront/index.php?area=game&AppId={{ capsules.bottom_right.appid }}">
    <img src="{{ BASE }}/storefront/images/capsules/{{ capsules.bottom_right.image }}"
         style="position: absolute; left: 301px; top: 352px; width: 289px; height: 158px; border: 0;"
         alt="Bottom right capsule" border="0"/>
  </a>
  {% endif %}
</div>
<br/>
</td>
</tr>
</table>
</td>
</tr>
</table>
</td>
<td bgcolor="#FFFFFF" style="width: 10px; height: 10px;" valign="top"><img src="img\storefront\sp_top_right.gif"/></td>
</tr>
<tr>
<td bgcolor="#FFFFFF"></td>
<td bgcolor="#FFFFFF"></td>
</tr>
<tr>
<td bgcolor="#FFFFFF" style="width: 10px; height: 10px;" valign="bottom"><img src="img\storefront\sp_bottom_left.gif"/></td>
<td bgcolor="#FFFFFF" style="width: 10px; height: 10px;" valign="bottom"><img src="img\storefront\sp_bottom_right.gif"/></td>
</tr>
</table>
